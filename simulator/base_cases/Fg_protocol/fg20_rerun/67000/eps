FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "67000";
    object      eps;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.3;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform 0.3;
    }
    outlet
    {
        type            calculated;
        value           uniform 0.3;
    }
    emptyWalls
    {
        type            empty;
    }
}


