FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      Ua;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];    // m/s

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {   
        type            fixedValue;
        value           uniform (0 0 {{ua}});
    }
    outlet
    {
        type            zeroGradient;
    }
    emptyWalls
    {
        type            empty;
    }
}

// ************************************************************************* //
