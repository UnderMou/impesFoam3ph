FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      Ua;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];    // m/s

internalField   uniform (0 0 0);

boundaryField
{

    walls
    {
        // type            zeroGradient;
        type            slip;

        // type            fixedNormalSlip;
        // value           uniform (0 0 0); // Optional initial value
    }

    emptyWalls
    {
        type            empty;
    }
}

