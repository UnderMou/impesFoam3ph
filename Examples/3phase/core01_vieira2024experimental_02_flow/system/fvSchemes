FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;
}

gradSchemes
{
    // default         cellMDLimited Gauss linear 0.333;
    // grad(p)         Gauss linear;
    // grad(fw)         Gauss linear;

    default         Gauss linear;

}

divSchemes
{
    default              none;
    // div(grad(p))        Gauss linear;

    // div(phi,fw)            Gauss limitedLinear 0.333;  
    // div(phi,fw)         Gauss upwind;
    // div(phi,fw)         Gauss linear;
    // div(phi,fw)         Gauss limitedLinear 0.1;

}

laplacianSchemes
{
    // default         Gauss linear orthogonal;

    // default         Gauss linear corrected;

    // default            Gauss linear limited 0.333;

    default         	Gauss linear corrected;

}

interpolationSchemes
{
    default         none;
    K               harmonic;

    // kra	            upwind phia;
    // krb         	upwind phib;
    // krc         	upwind phic;

    // kra             QUICK phia;
    // krb             QUICK phib;
    // krc             QUICK phic;

    kra             limitedLinear phia 0.75;
    krb             limitedLinear phib 0.75;
    krc             limitedLinear phic 0.75;

    // kra             TOPUS phia 0.1;
    // krb             TOPUS phib 0.1;
    // krc             TOPUS phic 0.1;

    flux(Ub)        linear;
    flux(Ua)        linear;
    flux(Uc)        linear;
    flux(U)         linear;
}

snGradSchemes
{
    // default         orthogonal;

    // default            limited 0.333;

    default         corrected;
}

fluxRequired
{
    default         no;
    p           ;
}


