FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      K;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1.579e-12; 

boundaryField
{
    walls
    {
        type            fixedValue;
        value           uniform 1.579e-12;
    }

    emptyWalls
    {
        type            empty;
    }
}