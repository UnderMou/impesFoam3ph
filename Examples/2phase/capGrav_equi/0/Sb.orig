FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      Sb;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1e-3; 

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    emptyWalls
    {
        type            empty;
    }
}


